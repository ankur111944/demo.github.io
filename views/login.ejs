<%- include("partials/header"); %>
<div class="container auth-form">
	<h1 class="text-center">Login</h1>
	<form class="form-group" action="/login" method="POST">
		<input class="form-control my-4" type="text" name="username" placeholder="Username">
		<input class="form-control my-4" type="password" name="password" placeholder="Password">
		<button class="btn btn-lg btn-outline-primary">Login</button>
	</form>
	<p>Don't have an account? <a href="/register">Sign up</a></p>
</div>
<%- include("partials/footer"); %>